<div class="container">
  <div class="row">
    <div class="">
      <h1>Login Page</h1>
      <%= form_for(:session, url: sessions_path) do |f| %>
        <%= f.label :email, 'EmailAddress' %>
        <%= f.text_field :email %>
        <%= f.label :password, 'Password' %>
        <%= f.password_field :password %>
        <%= f.submit 'Log in' %>
      <% end %>
    </div>
  </div>
</div>
